      "uuid": "f984de2f-aabe-4729-aa1b-448e55699e54",
      "id": "kakh",
      "name": "Database mock",
      "documentation": "",
      "value": "{\n  \"hideouts\": [\n    {{ setVar 'hideout_amount' (faker 'number.int' min=4 max=12) }}\n    {{#repeat @hideout_amount}}\n    {\n      \"guid\": \"{{ faker 'string.uuid' }}\",\n      \"name\": \"{{ faker 'word.words' }}\",\n      \"poeVersion\": {{ oneOf ( array '1' '2') }},\n      \"map\": \"{{ oneOf ( array 'Shrine Hideout' 'Trial of the Ancestors Hideout' 'Chaos Hideout' 'Baleful Hideout' ) }}\",\n      \"images\": [\n        {{ setVar 'img_amount' (faker 'number.int' min=2 max=4) }}\n        {{#repeat @img_amount}}\n        {\n          \"src\": \"{{ faker 'image.urlPicsumPhotos' }}\",\n          \"alt\": \"{{ faker 'lorem.sentence' }}\"\n        }\n        {{/repeat}}\n      ],\n      \"publishedAt\": \"{{ faker 'date.past' }}\",\n      \"lastUpdatedAt\": \"{{ faker 'date.recent' }}\",\n      \"authorUsername\": \"{{ faker 'internet.username' }}\",\n      \"timesDownloaded\": {{ faker 'number.int' min=3 max=100 }},\n      \"commentCount\": {{ faker 'number.int' min=2 max=40 }},\n      \"timesFavorited\": {{ faker 'number.int' min=7 max=200 }},\n      \"hasMTX\": {{ faker 'datatype.boolean' }},\n      \"rating\": {{ faker 'number.float' min=1 max=5 multipleOf=0.5 }},\n      \"description\": \"{{ faker 'lorem.sentence' }}\",\n      \"tags\": [\n        {{ setVar 'tag_amount' (faker 'number.int' min=2 max=4) }}\n        {{#repeat @tag_amount}}\n        \"{{ faker 'word.adjective' }}\",\n        {{/repeat}}\n      ],\n      \"changelogHistory\": [\n        {{ setVar 'log_amount' (faker 'number.int' min=1 max=4) }}\n        {{#repeat @log_amount}}\n        {\n          \"version\": \"{{ faker 'number.float' min=1 max=3 multipleOf=0.1 }}\",\n          \"publishedAt\": \"{{ faker 'date.past' }}\",\n          \"description\": \"{{ faker 'lorem.sentence' }}\",\n          \"fileURL\": \"/celestial-quarters.sample.hideout\"\n        }\n        {{/repeat}}\n      ]\n    }\n    {{/repeat}}\n  ],\n  \"tags\": [\"Example 1\", \"Example 2\"],\n  \"maps\": [\"Shrine Hideout\", \"Trial of the Ancestors Hideout\"]\n}"
    }
